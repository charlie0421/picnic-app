{
  "version": 2,
  "name": "ttja-prod",
  "ignoreCommand": "exit 0",

  "builds": [
    {
      "src": "packages/client/ttja_app/build/web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/download",
      "dest": "/download.html",
      "status": 200
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "outputDirectory": "packages/client/ttja_app/build/web"
}
