{
  "version": 2,
  "name": "picnic-prod",
  "ignoreCommand": "exit 0",

  "builds": [
    {
      "src": "packages/client/picnic_app/build/web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/download",
      "dest": "/build/web/download.html",
      "status": 200
    },
    {
      "src": "/privacy",
      "dest": "/build/web/private.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html",
      "status": 200
    }
  ],
  "outputDirectory": "packages/client/picnic_app/build/web"
}
