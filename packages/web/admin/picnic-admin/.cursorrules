{
  "templates": [
    {
      "pattern": "app/**/create/page.tsx",
      "template": "create-page.tsx"
    },
    {
      "pattern": "app/**/show/[id]/page.tsx",
      "template": "show-id-page.tsx"
    },
    {
      "pattern": "app/**/edit/[id]/page.tsx",
      "template": "edit-id-page.tsx"
    },
    {
      "pattern": "app/**/components/**/*.tsx",
      "template": "component.tsx"
    },
    {
      "pattern": "app/**/login/page.tsx",
      "template": "auth-login-page.tsx"
    },
    {
      "pattern": "app/**/register/page.tsx",
      "template": "auth-register-page.tsx"
    },
    {
      "pattern": "app/**/forgot-password/page.tsx",
      "template": "auth-forgot-password-page.tsx"
    },
    {
      "pattern": "app/admin_*/**/page.tsx",
      "template": "admin-page.tsx"
    },
    {
      "pattern": "app/**/layout.tsx",
      "template": "layout.tsx"
    },
    {
      "pattern": "app/not-found.tsx",
      "template": "not-found-page.tsx"
    },
    {
      "pattern": "app/artist/**/page.tsx",
      "template": "artist-page.tsx"
    },
    {
      "pattern": "app/media/**/page.tsx",
      "template": "media-page.tsx"
    }
  ]
}
